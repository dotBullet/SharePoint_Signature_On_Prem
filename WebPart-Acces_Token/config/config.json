{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "access-token-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/accessToken/AccessTokenWebPart.js",
          "manifest": "./src/webparts/accessToken/AccessTokenWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
    "path": "node_modules/jquery/dist/jquery.min.js",
    "globalName": "jQuery"
    },
    "bootstrap": {
    "path": "node_modules/bootstrap/dist/js/bootstrap.min.js",
    "globalName": "bootstrap"
    }
    },
  "localizedResources": {
    "AccessTokenWebPartStrings": "lib/webparts/accessToken/loc/{locale}.js"
  }
}
